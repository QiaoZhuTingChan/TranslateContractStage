		<id name="id" type="int">
			<column name="id" />
			<generator class="identity" />
		</id>
		<many-to-one name="contract" column="contract_id" class="CustomerContract"
			 />
		<property name="remark" type="java.lang.String">
			<column name="remark" />
		</property>
		<property name="gooutthereason" type="java.lang.String">
			<column name="goout_the_reason" />
		</property>
		<property name="gooutStartdate" type="java.sql.Date">
			<column name="goout_startdate" />
		</property>
		<property name="gooutEnddate" type="java.sql.Date">
			<column name="goout_enddate" />
		</property>
		<property name="presentAddress" type="java.lang.String">
			<column name="present_address" />
		</property>
		<property name="propertyAddress" type="java.lang.String">
			<column name="property_address" />
		</property>
		<property name="managetPhony" type="java.lang.String">
			<column name="managet_phony" />
		</property>
		<property name="companyName" type="java.lang.String">
			<column name="company_name" />
		</property>
		<property name="companyAddress" type="java.lang.String">
			<column name="company_address" />
		</property>
		<property name="companyManagerphony" type="java.lang.String">
			<column name="company_managerphony" />
		</property>
		<property name="companyStatus" type="java.lang.String">
			<column name="company_status" />
		</property>
		<property name="comprehensiveEvaluation" type="java.lang.String">
			<column name="comprehensive_evaluation" />
		</property>
		<property name="assessmentLevel" type="java.lang.String">
			<column name="assessment_level" />
		</property>
		<property name="createDate" type="java.sql.Timestamp">
			<column name="create_date" />
		</property>
		<property name="updateDate" type="java.sql.Timestamp">
			<column name="update_date" />
		</property>
		<property name="createUser" type="java.lang.String">
			<column name="create_user" />
		</property>
		<property name="updateUser" type="java.lang.String">
			<column name="update_user" />
		</property>

	</class>
</hibernate-mapping>
